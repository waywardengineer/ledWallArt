void getRGB(uint16_t hue, uint16_t sat, uint16_t val, uint16_t colors[3]) { 



	/* From http://www.kasperkamperman.com/blog/arduino/arduino-programming-hsb-to-rgb/
		convert hue, saturation and brightness ( HSB/HSV ) to RGB
		 The dim_curve is used only on brightness/value and on saturation (inverted).
		 This looks the most natural.      
	*/
	
	val = pgm_read_word_near(&(dim_curve[val]));
	sat = 4095-pgm_read_word_near(&(dim_curve[4095-sat]));
	
	unsigned long r = 0;
	unsigned long g = 0;
	unsigned long b = 0;
	unsigned long base;
	if (sat == 0) { // Acromatic color (gray). Hue doesn't mind.
		colors[0]=val;
		colors[1]=val;
		colors[2]=val;  
	} else  { 
		base = ((4095 - sat) * val)>>12;
		switch(hue/60) {
	case 0:
		r = val;
		g = (((val-base)*hue)/60)+base;
		b = base;
	break;
	
	case 1:
		r = (((val-base)*(60-(hue%60)))/60)+base;
		g = val;
		b = base;
	break;
	
	case 2:
		r = base;
		g = val;
		b = (((val-base)*(hue%60))/60)+base;
	break;
	
	case 3:
		r = base;
		g = (((val-base)*(60-(hue%60)))/60)+base;
		b = val;
	break;
	
	case 4:
		r = (((val-base)*(hue%60))/60)+base;
		g = base;
		b = val;
	break;
	
	case 5:
		r = val;
		g = base;
		b = (((val-base)*(60-(hue%60)))/60)+base;
	break;
		}
		colors[0]=r;
		colors[1]=g;
		colors[2]=b; 
	}   

}
